{
    "name": "Chargebacks Exceptions Extension",
    "description": "Make Chargebacks Great Again!",
    "version": "1.0",
    "manifest_version": 3,
    "permissions": ["storage", "activeTab", "scripting", "notifications"],
    "action": {
        "default_popup": "popup.html",
        "default_title": "Search for exceptions"
    },
    "content_scripts": [
        {
            "matches": ["https://cockpit.riskxint.com/admin/cb_cockpit*"],
            "js": ["papaparse.min.js","data.js","contentScript.js"],
            "run_at": "document_idle"
        }
    ]
}


